{
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "rules": {
    "strict": 1,
    "max-len": 0,
    "no-param-reassign": [2, { "props": false }],
    "no-unused-vars": [1, {"vars": "all", "args": "none"}],
    "eqeqeq": 1,
    "consistent-return": 1
  },
  "extends": "airbnb",
  "plugins": [
    "react"
  ]
}
